{
    "class": "AS3",
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.26.0",
        "id": "${id}",
        "label": "DemoCI/CD",
        "remark": "Generic HTTP applications",
        "Produccion_02": {
            "class": "Tenant",
            "vs_Application_http_8080": {
                "class": "Application",
                "template": "generic",
                "vsWAF_${VirtualServerName}_http_${VirtualPort}": {
                    "class": "Service_HTTP",
                    "virtualAddresses": [
                        "10.1.10.6"
                    ],
                    "pool": "${PoolName}",
                    "virtualPort": 8080,
                    "securityLogProfiles": [
                        {
                            "bigip": "/Common/Log all requests"
                        }
                    ],
                    "policyWAF": {
                        "use": "${NombrePoliticaWAF}"
                    }
                },
                "${PoolName}": {
                    "class": "Pool",
                    "monitors": [
                        "http"
                    ],
                    "members": [
                        {
                            "servicePort": 80,
                            "serverAddresses": [
                                "10.1.10.5"
                            ]
                        }
                    ]
                },
                "${NombrePoliticaWAF}": {
                    "class": "WAF_Policy",
                    "url": "${UrlWAF}",
                    "ignoreChanges": true
                }
            }
        }
    }
}